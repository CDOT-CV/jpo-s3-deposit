{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "type": "java",
            "name": "S3D Local",
            "request": "launch",
            "mainClass": "us.dot.its.jpo.ode.aws.depositor.AwsDepositor",
            "projectName": "jpo-aws-depositor",
            // arguments -i, -a, -n, -e, & -h are optional and not used in this launch configuration
            // replace <docker_host_ip> with the IP address of the docker host
            // replace <access key id> with the access key id of the AWS account
            // replace <access secret key> with the access secret key of the AWS account
            "args": [
                "-s", "<docker_host_ip>:9092",
                "-d", "s3",
                "-g", "<group>",
                "-k", "<key>",
                "-b", "<bucket>",
                "-r", "us-east-1",
                "-t", "topic.OdeBsmJson",
                "-x", "<access key id>",
            ],
            "env": {
                "AWS_ACCESS_KEY_ID": "<access key id>",
                "AWS_SECRET_KEY": "<access secret key>",
            }
        },
        {
            "type": "java",
            "name": "S3D CC",
            "request": "launch",
            "mainClass": "us.dot.its.jpo.ode.aws.depositor.AwsDepositor",
            "projectName": "jpo-aws-depositor",
            // arguments -i, -a, -n, -e, & -h are optional and not used in this launch configuration
            // replace <docker_host_ip> with the IP address of the docker host
            // replace <access key id> with the access key id of the AWS account
            // replace <access secret key> with the access secret key of the AWS account
            "args": [
                "-s", "<docker_host_ip>:9092",
                "-d", "s3",
                "-g", "<group>",
                "-k", "<key>",
                "-b", "<bucket>",
                "-r", "us-east-2",
                "-t", "topic.OdeBsmJson",
                "-x", "<access key id>",
            ],
            "env": {
                "AWS_ACCESS_KEY_ID": "<access key id>",
                "AWS_SECRET_KEY": "<access secret key>",
                "KAFKA_TYPE": "CONFLUENT",
                "CONFLUENT_KEY": "<confluent key>",
                "CONFLUENT_SECRET": "<confluent secret>"
            }
        }
    ]
}